@model IEnumerable<IdentityUser>

@{
    ViewData["Title"] = "Index";

}

<partial name="~/Views/Shared/Partials/_Header.cshtml" />

<a asp-controller="Account" asp-action="LogOut">Sign out</a>
      <section class="adminview">
        <div class="container">
          <h2>List of users</h2>
          @foreach (var user in Model)
          {
          <div class="user">
            <p>@user.Email</p>
            <p>@user.Id</p>
                <div class="editbuttons"><button><a asp-action="EditUserInfo" asp-route-Id="@user.Id">Edit user</a></button><button><a asp-action="ManageUserRole" asp-route-userId="@user.Id">Manage account role</a></button><button><a asp-action="Delete" asp-route-userId="@user.Id">Delete </a></button></div>
          </div>
          }
        </div>
      </section>